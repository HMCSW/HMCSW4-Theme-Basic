                    <li class="nav-item nav-item-spaced d-none d-lg-block nav-link text-red"><span class="text-white">DevMode activated</span>
                    </li>
                {% endif %}
                <li><a class="nav-link px-2 text-white" href="{{ url }}">{{ 'header.home' | language }}</a></li>
                <li class="nav-item dropdown">
