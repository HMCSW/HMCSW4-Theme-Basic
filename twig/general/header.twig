                <a href="{{ url }}/auth/logout" class="dropdown-item">
                  <i class="fa-regular fa-arrow-right-from-bracket"></i>
                  <span>{{ 'header.logout' | language }}</span>
                </a>
              </div>
            </li>
          </ul>
        {% endif %}
      </div>
      {% if guest == false %}
        <div class="order-lg-4 ml-lg-3">
          <a class="" href="#modal-profile" role="button" data-toggle="modal">
                        <span class="avatar rounded-circle">
                            <img loading="eager" title="{{ userData['name'] }}" alt="{{ userData['name'] }}"
                                 src="{{ userData['icon'] }}">
                        </span>
          </a>
        </div>
      {% endif %}
    </div>
  </nav>
</header>


{% if nightMode == "device" %}
  <script defer>
      if (window.matchMedia && window.matchMedia('(prefers-color-scheme: dark)').matches) {
          style_dark();
      } else {
          style_light();
      }

      window.matchMedia('(prefers-color-scheme: dark)').addEventListener('change', e => {
          const newColorScheme = e.matches ? 'dark' : 'light'
          if (newColorScheme === "dark") {
              style_dark();
          } else {
              style_light();
          }
      });

      function style_dark() {
          document.getElementById('navbar-main').setAttribute("class", "navbar navbar-main navbar-expand-lg shadow navbar-dark bg-dark");
          document.getElementById('navbar-logo').setAttribute("src", "{{ theme['icons']['banner']['dark']['svg'] }}");
      }

      function style_light() {
          document.getElementById('navbar-main').setAttribute("class", "navbar navbar-main navbar-expand-lg shadow navbar-light bg-white");
          document.getElementById('navbar-logo').setAttribute("src", "{{ theme['icons']['banner']['light']['svg'] }}");
      }
  </script>
{% endif %}

