              </div>
            {% endif %}
            <div class="d-flex">
              <div class="avatar-parent-child mx-auto">
                <img alt="{{ userData['name'] }}" src="{{ userData['icon'] }}" title="{{ userData['name'] }}"
                     loading="lazy" class="avatar avatar-xl rounded-circle">
                <span class="avatar-child avatar-badge bg-success"></span>
              </div>
            </div>
            <div class="text-center mb-4">
              <h6 class="h5 mb-0">{{ userData['name'] }}</h6>
              <span class="d-block text-muted">{{ userData['email'] }}</span>
              <span class="d-block text-muted">kn{{ userData['user_id'] }}</span>
            </div>

            <div class="mb-5">
              <div class="text-center mb-3">
                <span class="d-block h6 mb-1">{{ 'header.supportPin' | language }}</span>
                {% if config['support']['supportpin']['enabled'] %}
