                <div class="text-center">
                  <span class="d-block h6 mb-1">{{ 'header.balance' | language }}: {{ userData['credits'] | creditsToEuro }}{{ "symbol"|currency }}</span>
                </div>
                <a href="{{ url }}/cp/balance" class="btn btn-sm btn-primary">{{ 'header.balance.btn' | language }}</a>
              </div>
            </div>
            <div class="row justify-content-center">
            {% for badge in userData['badges'] %}
              <img alt="{{ badge['name'] }}" title="{{ badge['name'] }}" src="{{ badge['icon'] }}" class="avatar rounded-circle">
            {% endfor %}
            </div>
            <hr />
            <h6 class="text-center">{{ 'header.join' | language }}<br> {{ userData['created_at']|date("d.m.Y H:i") }}</h6>
          </div>
        </div>

        <div class="modal-footer">
