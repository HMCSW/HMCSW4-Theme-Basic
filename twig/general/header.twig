                {% endif %}
              </div>
            </div>

            <div class="card shadow-none border-0 bg-section-secondary mt-8">
              <div class="card-body pt-0 px-3 text-center">
                <div class="pb-4 px-5 mt-n6">
                  <img loading="lazy" title="{{ 'header.balance' | language }}" alt="{{ 'header.balance' | language }}"
                       src="{{ url }}/assets/img/svg/illustrations/illustration-15.svg" class="img-fluid img-center"/>
