<script async src="{{ url }}/include/js/notify.js"></script>
<script>
    function sendNotify(message, status){
        var body;
        if (status === "danger"){
            body = '<i class="fa fa-exclamation-triangle fs-xl pe-1 me-2"></i><div><b>'+message+'</b></div>'
        } else {
            body = '<i class="fa fa-check fs-xl pe-1 me-2"></i><div><b>'+message+'</b></div>'
        }

        props = {
            body:body,
            duration: 3000,
            type: status
        }

        if(!$('.navbar')[0]) {
            props.margin = "1rem";
        }

        $.showNotification(props)
    }
</script>
