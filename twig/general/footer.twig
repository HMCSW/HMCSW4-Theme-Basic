<script src="{{ url }}/assets/libs/@fancyapps/fancybox/dist/jquery.fancybox.js"></script>
<script src="{{ url }}/assets/libs/typed.js/lib/typed.min.js"></script>
<script src="{{ url }}/assets/libs/jquery-mask-plugin/dist/jquery.mask.min.js"></script>
