{{ '<link rel="stylesheet" media="screen" href="https://cdn.hosmatic.com/website/theme/assets/vendor/aos/dist/aos.css" type="text/css">'|addCSS }}
{{ '<link rel="stylesheet" media="screen" href="https://cdn.hosmatic.com/website/theme/assets/vendor/simplebar/dist/simplebar.min.css">'|addCSS }}
{{ '<script src="https://cdn.hosmatic.com/website/theme/assets/vendor/aos/dist/aos.js"></script>'|addJS }}
{{ '<script src="https://cdn.hosmatic.com/website/theme/assets/vendor/smooth-scroll/dist/smooth-scroll.polyfills.min.js"></script>'|addJS }}
{% include 'general/header.twig' %}
<!-- Stats-->
<section class="position-relative pt-sm-3 pt-md-5 mb-xl-3 mb-xxl-5">
    <div class="position-absolute top-0 end-0 overflow-hidden mt-n5 mt-md-0">
        <svg class="d-block mt-n5 mt-md-0 me-md-n5 me-xxl-0" width="1207" height="894" viewBox="0 0 1207 894"
             fill="currentColor" xmlns="http://www.w3.org/2000/svg" style="color: var(--ar-gray-100);">
            <circle cx="639" cy="255" r="639"></circle>
        </svg>
    </div>
    <div class="container pt-5 pb-lg-4 pb-xl-5 mt-5">
        <div class="row pt-xl-4">
            <!-- Sticky image-->
            <div class="col-md-5 col-lg-6 offset-xxl-1 order-md-2 position-relative mb-3 mb-md-0"
                 style="margin-top: -115px;">
                <div class="d-none d-md-block position-absolute bottom-0 end-0 text-uppercase fw-bold lh-1 pb-5 mb-4 pe-3"
                     style="color: var(--ar-gray-100); font-size: 180px; transform: translate3d(0,0,0);">
                    <div class="d-flex mb-lg-3" style="padding-left: 160px;">
                        <div data-aos="flip-right" data-aos-duration="700" data-aos-offset="400" data-aos-delay="150"
                             class="aos-init aos-animate">H
                        </div>
                        <div data-aos="flip-right" data-aos-duration="700" data-aos-offset="400" data-aos-delay="200"
                             class="aos-init aos-animate">O
                        </div>
                        <div data-aos="flip-right" data-aos-duration="700" data-aos-offset="400" data-aos-delay="250"
                             class="aos-init aos-animate">S
                        </div>
                        <div data-aos="flip-right" data-aos-duration="700" data-aos-offset="400" data-aos-delay="300"
                             class="aos-init aos-animate">M
                        </div>
                        <div data-aos="flip-right" data-aos-duration="700" data-aos-offset="400" data-aos-delay="350"
                             class="aos-init aos-animate">A
                        </div>
                        <div data-aos="flip-right" data-aos-duration="700" data-aos-offset="400" data-aos-delay="400"
                             class="aos-init aos-animate">T
                        </div>
                        <div data-aos="flip-right" data-aos-duration="700" data-aos-offset="400" data-aos-delay="400"
                             class="aos-init aos-animate">I
                        </div>
                        <div data-aos="flip-right" data-aos-duration="700" data-aos-offset="400" data-aos-delay="400"
                             class="aos-init aos-animate">C
                        </div>
                    </div>
                    <div class="d-flex">
                        <div data-aos="flip-right" data-aos-duration="700" data-aos-offset="300" data-aos-delay="150"
                             class="aos-init aos-animate">C
                        </div>
                        <div data-aos="flip-right" data-aos-duration="700" data-aos-offset="300" data-aos-delay="200"
                             class="aos-init aos-animate">O
                        </div>
                        <div data-aos="flip-right" data-aos-duration="700" data-aos-offset="300" data-aos-delay="250"
                             class="aos-init aos-animate">M
                        </div>
                    </div>
                </div>
                <div class="position-sticky zindex-3 top-0" style="padding-top: 115px;">
                    <div class="px-5 px-md-0 ps-xl-5 ms-lg-3">
                        {% include '/general/productCircle.twig' %}
                    </div>
                </div>
            </div>
            <!-- Text-->
            <div class="col-md-7 col-lg-6 col-xxl-5 order-md-1 position-relative zindex-3 text-center text-md-start pb-sm-3 pb-md-5 pt-4 mb-md-5 mt-2">
                <h1 class="display-3 text-uppercase mb-sm-4">
                    <span class="fw-medium">{{ 'site.home.top'|language|replace({"@name": '<span class="text-info fw-bold">'~name~'</span>'})|raw }}</span></h1>
                <div class="mx-auto mx-md-0" style="max-width: 400px;">
                    <p class="pb-2 pb-lg-0 mb-4 mb-lg-5">{{ 'site.home.top.sm'|language }}</p>
                    {% if guest == false %}
                        <a class="btn btn-outline-dark" href="{{ dashURL }}">{{ 'site.home.top.btn.cp' | language }}</a>
                    {% else %}
                        <a class="btn btn-outline-dark" href="{{ url }}/auth/register">{{ 'site.home.top.btn.register' | language }}</a>
                    {% endif %}
                    <div class="d-none d-xxl-block" style="height: 290px;"></div>
                    <div class="d-none d-md-block d-xxl-none" style="height: 240px;"></div>
                    <div class="d-md-none" style="height: 50px;"></div>
                    <div data-aos="fade-up" data-aos-duration="800" data-aos-offset="250"
                         data-disable-parallax-down="md" class="aos-init aos-animate">
                        <h2 class="h1 pb-sm-2 pb-md-3">{{ 'site.home.top2'|language }}</h2>
                        <p>{{ 'site.home.top2.sm'|language|replace({"@name": name}) }}</p>
                        <p></p>
                        <a
                                class="btn btn-link text-dark px-0" href="{{ url }}/about">{{ 'site.home.top2.btn'|language }}<i
                                    class="fa-duotone fa-arrow-right ms-2"></i></a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<section class="container pb-2 pb-sm-3 pb-md-4 pb-lg-5 mb-xl-3 mb-xxl-5">
    {% include 'general/themeAlert.twig' %}
    <div class="bg-light rounded-5 py-4 py-md-5 px-lg-5 aos-init aos-animate"  data-aos="fade-up" data-aos-duration="850"
         data-aos-offset="180" data-disable-parallax-down="md">

        <div class="row row-cols-1 row-cols-md-3 g-0">
            <div class="col d-md-flex justify-content-center text-center text-md-start position-relative">
                <div class="position-absolute top-50 end-0 translate-middle-y border-end" style="height: 60px;"></div>
                <div class="p-3 px-sm-0 py-sm-4">
                    <div class="h2 display-5 text-primary mb-0">{{ stats['user'] }}</div>
                    <span>{{ 'site.home.stats.user'|language }}</span>
                </div>
            </div>
            <div class="col d-md-flex justify-content-center text-center text-md-start position-relative">
                <div class="position-absolute top-50 end-0 translate-middle-y border-end d-none d-md-block"
                     style="height: 60px;"></div>
                <div class="p-3 px-sm-0 py-sm-4">
                    <div class="h2 display-5 text-primary mb-0">{{ stats['service'] }}</div>
                    <span>{{ 'site.home.stats.service'|language }}</span>
                </div>
            </div>
            <div class="col d-md-flex justify-content-center text-center text-md-start position-relative">
                <div class="p-3 px-sm-0 py-sm-4">
                    <div class="h2 display-5 text-primary mb-0">{{ stats['ticket'] }}</div>
                    <span>{{ 'site.home.stats.support'|language }}</span>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- Services grid-->
<section class="container py-5">
    <article
            class="row align-items-center justify-content-center card-hover pb-5 mb-md-2 mb-lg-3 mb-xl-4 aos-init aos-animate"
            data-aos="fade-up" data-aos-duration="600" data-aos-offset="280">
        <div class="col-md-4 offset-md-1 mb-4 mb-md-0">
            <img class="rounded-5 d-none d-md-block"
                 src="https://cdn.hosmatic.com/website/about/overview.svg"
                 width="300" alt="Image">

        </div>
        <div class="col-md-4 col-xl-4 aos-init aos-animate" data-aos="fade-up" data-aos-duration="850"
             data-aos-offset="180" data-disable-parallax-down="md">
            <div class="ms-md-4 ms-lg-5 ms-xxl-0">
                <div class="text-muted mb-2">{{ 'site.home.overview.badge'|language }}</div>
                <h2 class="mb-lg-4">{{ 'site.home.overview'|language }}</h2>
                <p class="mb-0 mb-md-1 mb-lg-3">{{ 'site.home.overview.description'|language|replace({"@name": name}) }}</p>
            </div>
        </div>
    </article>
    <article
            class="row align-items-center justify-content-center card-hover pb-5 mb-md-2 mb-lg-3 mb-xl-4 aos-init aos-animate"
            data-aos="fade-up" data-aos-duration="600" data-aos-offset="280">
        <div class="col-md-4 order-md-2 mb-4 mb-md-0 offset-md-1">
            <img class="rounded-5 d-none d-md-block"
                 src="https://cdn.hosmatic.com/website/about/control.svg"
                 width="300" alt="Image">
        </div>
        <div class="col-md-4 order-md-1 aos-init" data-aos="fade-up" data-aos-duration="850"
             data-aos-offset="180" data-disable-parallax-down="md">
            <div class="me-md-4 me-lg-5 me-xxl-0">
                <div class="text-muted mb-2 text-md-end">{{ 'site.home.control.badge'|language }}</div>
                <h2 class="mb-lg-4 text-md-end">{{ 'site.home.control'|language }}</h2>
                <p class="mb-0 mb-md-1 mb-lg-3 text-md-end">{{ 'site.home.control.description'|language|replace({"@name": name}) }}</p>
            </div>
        </div>
    </article>
</section>
<!-- CTA-->
<section class="bg-primary dark-mode py-5" data-aos="fade-up" data-aos-duration="600" data-aos-offset="300">
    <div class="container pt-lg-2 pt-xl-4 pt-xxl-5 pb-1 pb-sm-3">
        <div class="row pt-sm-3 pt-md-4">
            <div class="col-md-6 col-xl-5 offset-xl-1">
                <h2 class="display-4">{{ 'site.home.footer'|language }}</h2>
            </div>
            <div class="col-md-6 col-lg-5 col-xl-4 offset-lg-1">
                <p class="fs-xl pb-4 mb-2 mb-lg-3">{{ 'site.home.footer.sm'|language }}</p>
                <a class="btn btn-outline-dark" href="{{ url }}/auth/register">{{ 'site.home.footer.register'|language }}</a>
            </div>
        </div>
        <div class="d-none d-md-block text-center mt-n5">
            <svg class="text-warning ms-lg-5" width="171" height="97" viewBox="0 0 171 97" fill="currentColor"
                 xmlns="http://www.w3.org/2000/svg">
